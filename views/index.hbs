<div class="jumbotron" style="margin-bottom: 0px">
        <div class="container">


<h1 class="selecting" style="margin-top:2rem  " >{{title}}</h1>


{{# each products }}
  <div class="row">
    <div class="col-md-12">
      <div class="thumbnail">
        <div class="caption">
          <h3>{{ this.title }}</h3>
            <p class="description">{{{this.description}}}</p>
            <div class="clearfix">
            <div class="price pull-left"><i class="fa fa-tag" aria-hidden="true"></i> {{ this.price }} $ </div>
              <a href="/add/{{ this.id }}" class="button3 btn btn-dark btn-rounded pull-right" role="button"><i class="fa fa-shopping-cart" aria-hidden="true"></i> Add to cart</a>
            </div>
        </div>
      </div>
    </div>
  </div>
{{/each}}
</div>
</div>










